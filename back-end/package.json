{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Back end of QuizVerse",
  "main": "server.js",
  "scripts": {
    "test": "npx nyc --all mocha ./tests/ --recursive --timeout=3000 --exit"
  },
  "config": {
    "blanket": {
      "data-cover-only": "[routes/edit-set.js]",
      "data-cover-never": [
        "node_modules",
        "tests",
        "front-end"
      ],
      "data-cover-flags": {
        "debug": true
      },
      "loader": "./node-loaders/coffee-script",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    },
    "travis-cov": {
      "threshold": 50,
      "removeKey": "branchFcn"
    }
  },
  "keywords": [
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "QuizVerse"
  },
  "author": "Matt-J-Dong",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "blanket": "^1.2.3",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "coverage-node": "^8.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "istanbul": "^0.4.5",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-simple": "^0.5.6",
    "mocha": "^10.2.0",
    "mongodb": "^5.2.0",
    "mongoose": "^7.0.4",
    "multer": "^1.4.5-lts.1",
    "nyc": "^15.1.0",
    "travis-cov": "^0.2.5",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
